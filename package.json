{
  "name": "omf",
  "version": "1.0.12001",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "reinstall": "rm -rf node_modules && yarn install",
    "ios:dev-build": "rnsemup && xcodebuild -workspace ios/omf.xcworkspace -scheme omf clean archive",
    "ios:live-build": "rnsemup && xcodebuild -workspace ios/omf.xcworkspace -scheme 'omf (Production)' clean archive",
    "android:dev-build": "rnsemup && cd android && ENVFILE=.env ./gradlew assembleRelease && cd .. && yarn open-android-build-folder",
    "android:live-build": "rnsemup && cd android && ENVFILE=.env.production ./gradlew assembleRelease && cd .. && yarn open-android-build-folder",
    "android:live-bundle": "rnsemup && cd android && ENVFILE=.env.production ./gradlew bundleRelease && cd ..",
    "open-android-build-folder": "open android/app/build/outputs/apk/release",
    "pod": "cd ios && pod install && cd ..",
    "test": "jest \"__tests__/.*\\.test\\.(ts|js)\"",
    "test:coverage": "jest --coverage \"__tests__/.*\\.test\\.(ts|js|tsx)\"",
    "test:watch": "yarn test --verbose --watchAll",
    "test:update": "yarn test --verbose --coverage --updateSnapshot",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint . --ext .js,.ts,.tsx;stylelint 'src/**/*{.tsx,.ts}'",
    "lint:fix": "eslint . --fix --ext .js,.ts,.tsx",
    "fastlane:test": "rnsemup && yarn reinstall && cd android && fastlane test && cd ..",
    "fastlane:beta": "rnsemup && yarn reinstall && cd android && fastlane beta && cd ..",
    "fastlane:deploy": "rnsemup && yarn reinstall && cd android && fastlane deploy --env production && cd ..",
    "precommit": "lint-staged",
    "postinstall": "yarn pod",
    "android:dev-code-push": "appcenter codepush release-react -a jiggag/ironman-android -d Staging",
    "android:live-code-push": "appcenter codepush release-react -a jiggag/ironman-android -d Production"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/datetimepicker": "^2.3.1",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-firebase/admob": "^7.1.2",
    "@react-native-firebase/app": "^8.2.0",
    "@react-native-firebase/messaging": "^7.1.3",
    "@react-native-seoul/kakao-login": "^2.4.1",
