{
  "name": "omf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "reinstall": "rm -rf node_modules && yarn install",
    "ios:dev-build": "xcodebuild -workspace ios/omf.xcworkspace -scheme omf clean archive",
    "ios:live-build": "xcodebuild -workspace ios/omf.xcworkspace -scheme 'omf (Production)' clean archive",
    "android:dev-build": "cd android && ENVFILE=.env ./gradlew assembleRelease && cd ..",
    "android:live-build": "cd android && ENVFILE=.env.production ./gradlew assembleRelease && cd ..",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "@slack/client": "^5.0.2",
    "axios": "^0.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "q": "^1.5.1",
    "react": "^16.9.0",
    "react-native": "^0.60.5",
    "react-native-config": "^0.11.7",
    "react-native-datepicker": "^1.7.2",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-kakao-logins": "^1.3.8",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-reanimated": "^1.4.0",
    "react-native-router-flux": "^4.0.6",
    "react-native-sensitive-info": "^5.5.3",
    "react-native-ui-lib": "^3.34.3",
    "react-navigation": "3.11.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "slack-node": "^0.1.8",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.2",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-test-renderer": "16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/__tests__/**/*.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  }
}
