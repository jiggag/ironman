    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^1.19.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "redux-devtools-extension": "^2.13.8",
    "stylelint": "^13.3.0",
