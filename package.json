{
  "name": "omf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "rm -rf ios/build && react-native run-ios",
    "android": "react-native run-android",
    "reinstall": "rm -rf node_modules && yarn install",
    "ios:dev-build": "xcodebuild -workspace ios/omf.xcworkspace -scheme omf clean archive",
    "ios:live-build": "xcodebuild -workspace ios/omf.xcworkspace -scheme 'omf (Production)' clean archive",
    "android:dev-build": "cd android && ENVFILE=.env ./gradlew assembleRelease && cd ..",
    "android:live-build": "cd android && ENVFILE=.env.production ./gradlew assembleRelease && cd ..",
    "android:live-bundle": "cd android && ENVFILE=.env.production ./gradlew bundleRelease && cd ..",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "server": "node ./common/server.js",
    "intall:server": "yarn add express dotenv mysql moment slack-node @slack/client jsonwebtoken",
    "fastlane:test": "cd android && fastlane test && cd ..",
    "fastlane:beta": "cd android && fastlane beta && cd ..",
    "fastlane:deploy": "cd android && fastlane deploy && cd ..",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.5",
    "axios": "^0.19.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "q": "^1.5.1",
    "react": "^16.9.0",
    "react-native": "^0.60.5",
    "react-native-chart-kit": "file:./src/lib/react-native-chart-kit/",
    "react-native-config": "^0.11.7",
    "react-native-datepicker": "^1.7.2",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-kakao-logins": "^1.3.8",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-reanimated": "^1.4.0",
    "react-native-sensitive-info": "^5.5.3",
    "react-native-svg": "^9.13.6",
    "react-native-ui-lib": "^3.34.3",
    "react-navigation": "3.11.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.5.5",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.2",
    "@types/styled-components": "^4.4.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "codecov": "^3.6.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "metro-react-native-babel-preset": "^0.55.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ],
    "cacheDirectory": ".jest/cache",
    "testMatch": [
      "**/__tests__/**/*.js"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
