  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.2",
