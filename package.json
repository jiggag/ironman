{
  "name": "omf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "rm -rf ios/build && react-native run-ios",
    "android": "react-native run-android",
    "reinstall": "rm -rf node_modules && yarn install",
    "ios:dev-build": "rnsemup && xcodebuild -workspace ios/omf.xcworkspace -scheme omf clean archive",
    "ios:live-build": "rnsemup && xcodebuild -workspace ios/omf.xcworkspace -scheme 'omf (Production)' clean archive",
    "android:dev-build": "rnsemup && cd android && ENVFILE=.env ./gradlew assembleRelease && cd .. && open-android-build-folder",
    "android:live-build": "rnsemup && cd android && ENVFILE=.env.production ./gradlew assembleRelease && cd .. && open-android-build-folder",
    "android:live-bundle": "rnsemup && cd android && ENVFILE=.env.production ./gradlew bundleRelease && cd ..",
    "open-android-build-folder": "open android/app/build/outputs/apk/release",
    "pod": "cd ios;pod install;cd ..",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint . --ext .js,.ts,.tsx;stylelint 'src/**/*{.tsx,.ts}'",
    "lint:fix": "eslint . --fix --ext .js,.ts,.tsx",
    "intall:server": "yarn add express dotenv mysql moment slack-node @slack/client jsonwebtoken",
    "fastlane:test": "rnsemup && cd android && fastlane test && cd ..",
    "fastlane:beta": "rnsemup && cd android && fastlane beta && cd ..",
    "fastlane:deploy": "rnsemup && cd android && fastlane deploy --env production && cd ..",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^2.3.1",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-firebase/admob": "^7.1.2",
    "@react-native-firebase/app": "^7.1.4",
    "@react-native-firebase/messaging": "^7.1.3",
    "@react-native-seoul/kakao-login": "^2.4.1",
    "@react-navigation/native": "^5.1.5",
    "@react-navigation/stack": "^5.2.10",
    "@reduxjs/toolkit": "^1.2.5",
    "@sentry/react-native": "^1.3.6",
    "axios": "^0.19.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "q": "^1.5.1",
    "react": "16.11.0",
    "react-native": "0.62.0",
    "react-native-chart-kit": "file:./src/lib/react-native-chart-kit/",
    "react-native-config": "^0.11.7",
    "react-native-datepicker": "^1.7.2",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-semver-update": "^1.1.2",
    "react-native-sensitive-info": "^5.5.3",
    "react-native-simple-toast": "^1.0.0",
    "react-native-svg": "^9.13.6",
    "react-native-ui-lib": "^3.34.3",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.2",
    "@types/styled-components": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@welldone-software/why-did-you-render": "^4.0.6",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "codecov": "^3.6.5",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-react-native": "^3.7.0",
    "eslint-plugin-react-redux": "^3.0.3",
    "eslint-plugin-redux-saga": "^1.1.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.11.0",
    "redux-devtools-extension": "^2.13.8",
    "stylelint": "^13.3.0",
    "stylelint-config-react-native-styled-components": "^0.3.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.2.0",
    "typescript": "^3.5.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "yarn lint:fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ],
    "cacheDirectory": ".jest/cache",
    "testMatch": [
      "**/__tests__/**/*.js"
    ],
    "setupFiles": [
      "./jest.setup.js",
      "./node_modules/react-native-ui-lib/jest-setup.js"
    ]
  }
}
