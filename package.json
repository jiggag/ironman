    "react-native-semver-update": "^1.1.2",
    "react-native-sensitive-info": "^5.5.3",
    "react-native-simple-toast": "^1.0.0",
    "react-native-svg": "^9.13.6",
    "react-native-ui-lib": "^3.34.3",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
